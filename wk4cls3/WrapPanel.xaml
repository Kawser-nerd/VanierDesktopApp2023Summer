<Window x:Class="wk4cls3.WrapPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wk4cls3"
        mc:Ignorable="d"
        Title="WrapPanel" Height="450" Width="800">
    <WrapPanel Margin="68,68,68,62">
        <Button x:Name="button" Content="Button" RenderTransformOrigin="0.723,1.964" Width="168" Margin="10,10,10,8" Padding="10,10,10,10"/>
        <TextBox x:Name="textBox" TextWrapping="Wrap" Text="TextBox" Width="120"/>
        <TabControl x:Name="tabControl" Width="189">
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <Rectangle Height="50" Stroke="Black" Width="100"/>
        <Button x:Name="button1" Content="Button" Width="77"/>
        <Button x:Name="button2" Content="Button" Height="39" Width="117"/>
        <Label x:Name="label" Content="Label" Width="88"/>
        <Button x:Name="button3" Content="Button" Width="97"/>
    </WrapPanel>

</Window>
